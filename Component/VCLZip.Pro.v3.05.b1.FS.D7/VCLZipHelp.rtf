\par \cf0\b0\fs16 
\par \pard\keepn\tx1440\tx2880\tx4320\tx5760\tx7200\tx8640\cf2\ul See also\cf3\ulnone{\v %Scribble2396}\tab\cf2\strike Example\cf3\strike0{\v %Scribble2397}\cf5\cf0\b\fs22 
